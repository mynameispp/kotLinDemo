<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vipyoung.testkotlin">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name="com.vipyoung.app2.application.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.vipyoung.app2.view.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.vipyoung.app2.view.function_test.ContrastActivity"/>
        <activity android:name="com.vipyoung.app2.view.function_test.CoroutineActivity"/>
        <activity android:name="com.vipyoung.app2.view.function_test.GenericityActivity" />
        <activity android:name="com.vipyoung.app2.view.function_test.list.ListActivity" />

        <meta-data
            android:name="com.bumptech.glide.samples.flickr.FlickrGlideModule"
            android:value="MyAppGlideModule" />
    </application>

</manifest>